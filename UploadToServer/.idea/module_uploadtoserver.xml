<?xml version="1.0" encoding="UTF-8"?>
<project name="module_uploadtoserver" default="compile.module.uploadtoserver">
  <dirname property="module.uploadtoserver.basedir" file="${ant.file.module_uploadtoserver}"/>
  
  <property name="module.jdk.home.uploadtoserver" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.uploadtoserver" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.uploadtoserver" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.uploadtoserver" value="-encoding UTF-8 -source 1.6 ${compiler.args}"/>
  
  <property name="uploadtoserver.output.dir" value="undefined"/>
  <property name="uploadtoserver.testoutput.dir" value="undefined"/>
  
  <path id="uploadtoserver.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="uploadtoserver.module.production.classpath">
    <path refid="${module.jdk.classpath.uploadtoserver}"/>
  </path>
  
  <path id="uploadtoserver.runtime.production.module.classpath"/>
  
  <path id="uploadtoserver.module.classpath">
    <path refid="${module.jdk.classpath.uploadtoserver}"/>
  </path>
  
  <path id="uploadtoserver.runtime.module.classpath"/>
  
  
  <patternset id="excluded.from.module.uploadtoserver">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.uploadtoserver">
    <patternset refid="excluded.from.module.uploadtoserver"/>
  </patternset>
  
  
  <target name="compile.module.uploadtoserver" depends="compile.module.uploadtoserver.production,compile.module.uploadtoserver.tests" description="Compile module UploadToServer"/>
  
  <target name="compile.module.uploadtoserver.production" depends="register.custom.compilers" description="Compile module UploadToServer; production classes"/>
  
  <target name="compile.module.uploadtoserver.tests" depends="register.custom.compilers,compile.module.uploadtoserver.production" description="compile module UploadToServer; test classes" unless="skip.tests"/>
  
  <target name="clean.module.uploadtoserver" description="cleanup module">
    <delete dir="${uploadtoserver.output.dir}"/>
    <delete dir="${uploadtoserver.testoutput.dir}"/>
  </target>
</project>